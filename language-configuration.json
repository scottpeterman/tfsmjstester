{
    "comments": {
      "lineComment": "#"
    },
    "brackets": [
      ["{", "}"],
      ["(", ")"],
      ["[", "]"]
    ],
    "autoClosingPairs": [
      { "open": "{", "close": "}" },
      { "open": "[", "close": "]" },
      { "open": "(", "close": ")" },
      { "open": "${", "close": "}" }
    ],
    "surroundingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["\"", "\""],
      ["'", "'"]
    ],
    "folding": {
      "markers": {
        "start": "^\\s*Start",
        "end": "^\\s*End"
      }
    }
  }